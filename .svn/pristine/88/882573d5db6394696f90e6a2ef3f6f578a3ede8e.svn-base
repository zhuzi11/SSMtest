											       <select  class="form-control"  id="state" name="state">
											           <option value="1">启用</option>
													   <option value="0">禁用</option>  
											       </select>
											    </div>
											  </div>
											 
											  <div class="form-group">
											    <div class="col-sm-offset-2 col-sm-10">
											      <button type="button" class="btn btn-default">保存</button>
											    </div>
											  </div>
											</form> 
						        </div>
						 </div>

</body>
