package com.wisezone.exception;

/**
 * 用户名和密码错误的异常
 * @author Administrator
 *
 */
 
public class UserNameAndPasswordErrorException extends RuntimeException {
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	public UserNameAndPasswordErrorException() {
		super("用户名或者密码错误");	 
	}

	public UserNameAndPasswordErrorException(String message, Throwable cause) {
		super(message, cause);
		 
	}

	public UserNameAndPasswordErrorException(String message) {
		super(message);
	}

	
}
